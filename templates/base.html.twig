		</div>

		<div data-controller="hello" class="header under-construction">Under Construction</div>

		{% block body %}{% endblock %}

		{{ encore_entry_script_tags('app') }}
		{{ render_esi(path('ch_cookie_consent.show_if_cookie_consent_not_set', { 'locale' : app.request.locale })) }}

